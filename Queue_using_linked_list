#include <stdio.h>
#include <stdlib.h>

struct node {
    int data;
    struct node *next;
};

struct node *front = NULL, *rear = NULL;

/* Enqueue operation */
void enqueue(int x) {
    struct node *new = (struct node*)malloc(sizeof(struct node));
    new->data = x;
    new->next = NULL;

    if (rear == NULL) {
        front = rear = new;
        return;
    }
    rear->next = new;
    rear = new;
}

/* Dequeue operation */
void dequeue() {
    if (front == NULL) {
        printf("Queue Underflow\n");
        return;
    }
    struct node *temp = front;
    printf("Deleted element: %d\n", temp->data);
    front = front->next;

    if (front == NULL)
        rear = NULL;

    free(temp);
}

/* Display queue */
void display() {
    struct node *temp = front;
    if (temp == NULL) {
        printf("Queue Empty\n");
        return;
    }
    while (temp) {
        printf("%d ", temp->data);
        temp = temp->next;
    }
    printf("\n");
}

int main() {
    enqueue(10);
    enqueue(20);
    enqueue(30);
    display();
    dequeue();
    display();
    return 0;
}
